<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#007AFF"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/content_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#3B8AFD"
        android:gravity="center">

        <View
            android:id="@+id/_bg__iphone_x__xs__11_pro___19"
            android:layout_width="match_parent"
            android:layout_height="820dp" />

        <ImageView
            android:id="@+id/rectangle_56"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="rectangle_56" />

        <TextView
            android:id="@+id/kaun"
            android:layout_width="wrap_content"
            android:layout_height="89dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"

            android:fontFamily="@font/architectsdaughter_regular"
            android:gravity="center"
            android:letterSpacing="0.11"
            android:text="@string/kaun_string"
            android:textColor="#ffffff"
            android:textSize="55sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/get_started"
            android:layout_width="90dp"
            android:layout_height="18dp"
            android:layout_marginLeft="-332dp"
            android:layout_marginTop="660.64501953125dp"
            android:fontFamily="Roboto"
            android:text="@string/get_started_string"
            android:textColor="@color/get_started_color"
            android:textSize="13sp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_horizontal"
            android:layout_alignParentBottom="true">
            <TextView
                android:id="@+id/made_in"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:fontFamily="@font/montserratmedium"
                android:gravity="center_horizontal"
                android:text="Made in "
                android:textColor="#FFFFFF"
                android:textSize="19sp" />

            <TextView
                android:id="@+id/made_ins"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:fontFamily="@font/montserratmedium"
                android:text="India"
                android:textColor="@color/white"
                android:textSize="19sp" />
        </LinearLayout>




    </RelativeLayout>


</LinearLayout>